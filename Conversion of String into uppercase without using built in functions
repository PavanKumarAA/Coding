#include<stdio.h>
void main()
{
    int i;
    char str[20];
    printf("enter the string");
    scanf("%s",str);
    int length;
    for(length=0;str[length];length++);
    for(int i=0;i<length;i++)
    {
        if(str[i]>='a'&& str[i<='z'])

    str[i]=(int)str[i]-32;
    }
    printf("%s",str);
}
